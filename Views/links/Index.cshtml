@model IEnumerable<appp.Models.link>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.webLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.linkRole)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.webLink)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.linkRole)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.key }) |
            @Html.ActionLink("Details", "Details", new { id=item.key }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.key })
        </td>
    </tr>
}

</table>
